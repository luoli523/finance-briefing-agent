{
  "name": "finance-briefing-agent",
  "version": "1.0.0",
  "description": "AI-powered finance briefing generator",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "collect": "tsx src/scripts/collect.ts",
    "collect:yahoo": "tsx src/scripts/collect-yahoo.ts",
    "collect:finnhub": "tsx src/scripts/collect-finnhub.ts",
    "collect:fred": "tsx src/scripts/collect-fred.ts",
    "collect:sec": "tsx src/scripts/collect-sec.ts",
    "collect:rss": "tsx src/scripts/collect-rss.ts",
    "analyze": "tsx src/scripts/analyze.ts",
    "analyze:intelligent": "tsx src/scripts/analyze-intelligent.ts",
    "analyze:enhanced": "tsx src/scripts/analyze-enhanced.ts",
    "generate": "tsx src/scripts/generate.ts",
    "generate:intelligent": "tsx src/scripts/generate-intelligent.ts",
    "workflow:intelligent": "tsx src/scripts/run-intelligent-workflow.ts",
    "verify:config": "tsx src/scripts/verify-config.ts",
    "generate:decision": "tsx src/scripts/generate-decision-report.ts",
    "generate:decision-enhanced": "tsx src/scripts/generate-decision-enhanced.ts",
    "workflow:decision": "tsx src/scripts/collect.ts && tsx src/scripts/analyze.ts && tsx src/scripts/generate-decision-report.ts",
    "workflow:decision-enhanced": "tsx src/scripts/collect.ts && tsx src/scripts/analyze-intelligent.ts && tsx src/scripts/analyze-enhanced.ts && tsx src/scripts/generate-decision-enhanced.ts",
    "generate:infographic": "tsx src/scripts/generate-infographic.ts",
    "generate:infographic-enhanced": "tsx src/scripts/generate-infographic-enhanced.ts",
    "generate:decision-v2": "tsx src/scripts/generate-decision-v2.ts",
    "workflow:v2": "tsx src/scripts/collect.ts && tsx src/scripts/analyze.ts && tsx src/scripts/generate-decision-v2.ts",
    "workflow:all": "npm run workflow:decision-enhanced && npm run generate:infographic-enhanced",
    "generate:pro": "tsx src/scripts/generate-professional-briefing.ts",
    "workflow:pro": "tsx src/scripts/collect.ts && tsx src/scripts/analyze.ts && tsx src/scripts/generate-professional-briefing.ts",
    "daily": "npm run workflow:pro"
  },
  "keywords": [
    "finance",
    "briefing",
    "ai",
    "stock",
    "market"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^17.2.3",
    "https-proxy-agent": "^7.0.6",
    "rss-parser": "^3.13.0",
    "yahoo-finance2": "^3.11.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
